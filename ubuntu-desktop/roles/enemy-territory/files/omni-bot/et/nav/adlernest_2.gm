global Map =
{
	Debug = 0, // please set to zero before distributing your script
	ShowMovers = false,
	Talk = true,
	talk = 1,
	DefendingTeam = TEAM.AXIS,
	
	Allied_CP_Dyno = 0,
	Door_controls_Dyno = 0,

	BlastDoorOpen = 0,
	CP_build = 0,
	Docs_taken = 0,
	DoorControlDestroyed = 0,
	AlliesInTrigger1 = 0,
	EngInTrigger1 = 0,
	AlliesInTrigger2 = 0,
	EngInTrigger2 = 0,
	AlliesInTrigger3 = 0,
	EngInTrigger3 = 0,
	PipeBar1_Exploded = false,
