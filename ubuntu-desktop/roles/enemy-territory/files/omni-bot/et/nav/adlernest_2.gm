				SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_Axis_DoorControls_5" );
				SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_Axis_DoorControls_6" );
				SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_Axis_DoorControls_8" );
