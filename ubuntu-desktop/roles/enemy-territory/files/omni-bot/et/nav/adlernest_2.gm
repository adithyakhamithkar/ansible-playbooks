			{
				if ( Map.AlliesInTrigger1 > 0 or Map.AlliesInTrigger2 > 0 or Map.AlliesInTrigger3 > 0 )
				{
					SetAvailableMapGoals( TEAM.AXIS, false, ".*_Axis_DoorControls_.*" );
					
