global OnBotJoin = function( bot )
{
	bot.TargetBreakableDist = 60.0;
	bot.MaxViewDistance = 2400;
	w = bot.GetWeapon(WEAPON.KNIFE);
	w.PrimaryFire.SetDesirabilityRange(0, 48, 0.2);
};
