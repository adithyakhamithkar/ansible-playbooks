		if ( rand == 1 )
			{ Util.SetGoalPosition( 23686,-5761,229, Map.Plant_Axis_Helicopter ); }
		else if ( rand == 2 )
			{ Util.SetGoalPosition( 22629,-6971,184, Map.Plant_Axis_Helicopter ); }
		else if ( rand == 3 )
			{ Util.SetGoalPosition( 23696,-1086,184, Map.Plant_Axis_Helicopter ); }
