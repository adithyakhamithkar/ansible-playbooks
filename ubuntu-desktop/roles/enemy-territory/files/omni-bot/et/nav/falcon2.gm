	falcon_airlock_Destroyed = function( trigger )
	{
		if ( TestMap )
			{ return; }

		Map.falcon_airlock_Dyno = 0;

		SetAvailableMapGoals( TEAM.AXIS, true, "CAMP_Falcon.*" );
