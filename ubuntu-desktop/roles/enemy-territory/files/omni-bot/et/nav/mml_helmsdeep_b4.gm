		{
			navigate = function(_this)
			{
				if ( !Map.Turner )
				{
					if ( !Map.WellswitchInfo ) {
						Map.WellswitchInfo = Util.WpNameInfo("Wellswitch");
					}

					_this.AddAimRequest(Priority.High, "facing", Map.WellswitchInfo.facing);

					while( !Map.Turner )
					{
						_this.Bot.HoldButton(BTN.USE, 0.3);
						yield();
					}
				}
				sleep(1);
			},
		},
