		}

		if( Map.citymg42_status == 0 )
		{
			SetAvailableMapGoals( TEAM.ALLIES, true, "SWITCH_CityMG42" );
			SetAvailableMapGoals( TEAM.AXIS, false, "SWITCH_CityMG42" );
		}

