		if( Map.citymg42_status == 1 )
		{
			SetAvailableMapGoals( TEAM.ALLIES, false, "SWITCH_CityMG42" );
			SetAvailableMapGoals( TEAM.AXIS, true, "SWITCH_CityMG42" );
		}

