	},

	city_Allies_Captured = function( trigger )
	{
		if ( TestMap )
			{ return; }

		Map.city = 1;

		SetAvailableMapGoals( TEAM.ALLIES, true, "DEFEND_cityD.*" );

		if( Map.citymg42_status == 1 )
		{
			SetAvailableMapGoals( TEAM.ALLIES, true, "MOUNTMG42_city_mg" );
