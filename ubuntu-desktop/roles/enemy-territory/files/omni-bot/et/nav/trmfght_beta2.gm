		{
			SetAvailableMapGoals( TEAM.AXIS, false, "DEFEND_cappoint.*" );
			SetAvailableMapGoals( TEAM.AXIS, true, "RIDE_Tram" );
			Util.MapDebugPrint( GetEntName(ent) + " exits at_church" );
		},
