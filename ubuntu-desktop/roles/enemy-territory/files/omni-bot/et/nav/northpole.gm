	Tram_Waiting = function( trigger )
	{
		vec = ToVector( trigger.Action );
		if ( vec.x == -100 )
		{
			Map.TramStatus = "waiting";
			Map.Switches.tramswitch1.Enabled = true;
			Util.MapDebugPrint("tram waiting");
		}
