	East_Anti_Tank_Door_Planted = function( trigger )
	{
		if ( TestMap )
			{ return; }
		
		Map.East_Anti_Tank_Door_Dyno += 1;
		
		SetAvailableMapGoals( TEAM.AXIS, false, "DEFUSE_East_Anti_Tank_Door_.*" );
		
		Util.MapDebugPrint( "^5East_Anti_Tank_Door_Planted" );
	},
	
