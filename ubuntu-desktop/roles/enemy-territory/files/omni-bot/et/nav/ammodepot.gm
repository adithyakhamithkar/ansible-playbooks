			SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_Axis_SandBarrier_05" );
			SetAvailableMapGoals( TEAM.AXIS, true, "DEFEND_Axis_SandBarrier_06" );
			SetAvailableMapGoals( TEAM.AXIS, true, "SMOKEBOMB_Axis_SandBarrier_07" );
