	OnTrigger( "The Rhine Bridge has been repaired.", Map.Rhine_Bridge_Built );
	OnTrigger( "MISSING_STRING", Map.Stairs_Built );
	OnTrigger( "MISSING_STRING", Map.Bunker_Door_Destroyed );
